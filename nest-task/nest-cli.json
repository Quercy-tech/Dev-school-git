{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-task/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-task/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-task",
  "projects": {
    "nest-task": {
      "type": "application",
      "root": "apps/nest-task",
      "entryFile": "main",
      "sourceRoot": "apps/nest-task/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-task/tsconfig.app.json"
      }
    },
    "module": {
      "type": "application",
      "root": "apps/module",
      "entryFile": "main",
      "sourceRoot": "apps/module/src",
      "compilerOptions": {
        "tsConfigPath": "apps/module/tsconfig.app.json"
      }
    }
  }
}